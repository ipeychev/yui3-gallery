            <span class="k">this</span><span class="p">.</span><span class="nx">fire</span><span class="p">(</span> <span class="nx">ITEMERESIZED</span><span class="p">,</span> <span class="p">{</span>
                <span class="s1">&#39;item&#39;</span><span class="o">:</span> <span class="nx">item</span>
            <span class="p">});</span>

            <span class="k">if</span><span class="p">(</span> <span class="nx">notifyOthers</span> <span class="p">){</span>
