        <span class="nx">dd</span><span class="p">.</span><span class="nx">on</span>   <span class="p">(</span> <span class="s2">&quot;drag:drophit&quot;</span><span class="p">,</span> <span class="nx">Y</span><span class="p">.</span><span class="nx">bind</span><span class="p">(</span> <span class="k">this</span><span class="p">.</span><span class="nx">_onDropHit</span><span class="p">,</span>    <span class="k">this</span><span class="p">,</span> <span class="nx">dd</span> <span class="p">)</span> <span class="p">);</span>
