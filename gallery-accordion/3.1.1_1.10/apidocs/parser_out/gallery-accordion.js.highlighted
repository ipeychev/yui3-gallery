            <span class="nx">anim</span><span class="p">.</span><span class="nx">set</span><span class="p">(</span> <span class="s2">&quot;duration&quot;</span><span class="p">,</span> <span class="nx">animSettings</span><span class="p">.</span><span class="nx">duration</span> <span class="o">||</span> <span class="nx">accAnimationSettings</span><span class="p">.</span><span class="nx">duration</span> <span class="p">);</span>
