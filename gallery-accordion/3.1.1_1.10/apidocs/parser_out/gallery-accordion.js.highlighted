            <span class="nx">anim</span><span class="p">.</span><span class="nx">on</span><span class="p">(</span> <span class="s2">&quot;end&quot;</span><span class="p">,</span> <span class="nx">Y</span><span class="p">.</span><span class="nx">bind</span><span class="p">(</span> <span class="k">this</span><span class="p">.</span><span class="nx">_onExpandComplete</span><span class="p">,</span> <span class="k">this</span><span class="p">,</span> <span class="nx">item</span><span class="p">,</span> <span class="nx">notifyOthers</span> <span class="p">)</span> <span class="p">);</span>

