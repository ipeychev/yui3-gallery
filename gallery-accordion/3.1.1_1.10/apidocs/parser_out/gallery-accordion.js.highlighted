    <span class="nx">_processExpanding</span><span class="o">:</span> <span class="kd">function</span><span class="p">(</span> <span class="nx">item</span><span class="p">,</span> <span class="nx">height</span><span class="p">,</span> <span class="nx">forceSkipAnimation</span> <span class="p">){</span>
